<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple/DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>slackthemer</string>
    <key>ProgramArguments</key>
    <array>
      <string>~/Downloads/themer_mac.sh</string>
    </array>    
    <key>RunAtLoad</key>
    <true/> 
    <key>StandardOutPath</key>
    <string>~/Desktop/slackthemerlog.log</string>
    <key>StandardErrorPath</key>
    <string>~/Desktop/slackthemererror.log</string>  
    <key>StartInterval</key>
    <integer>60</integer>    
  </dict>
</plist>
